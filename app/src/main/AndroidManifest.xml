<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="boonleng94.iguide">

    <application
        android:name="boonleng94.iguide.CloudController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="boonleng94.iguide.MainIndoorActivity">
        </activity>

        <activity
            android:name="boonleng94.iguide.SplashActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="boonleng94.iguide.STTTest">
        </activity>

        <activity android:name="boonleng94.iguide.MainCompassActivity">
        </activity>
    </application>

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
</manifest>
